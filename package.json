{
  "name": "react-sortable-tree",
  "version": "0.1.21",
  "description": "Drag-and-drop sortable component for nested data and hierarchies",
  "scripts": {
    "build": "npm run clean && cross-env NODE_ENV=production TARGET=umd webpack --bail",
    "build:demo": "npm run clean:demo && cross-env NODE_ENV=production TARGET=demo webpack --bail",
    "clean": "rimraf dist/umd",
    "clean:demo": "rimraf build",
    "start": "cross-env NODE_ENV=development TARGET=development webpack-dev-server --inline --hot",
    "external-nodes-demo": "cross-env NODE_ENV=development TARGET=developmentExternal webpack-dev-server --inline --hot",
    "lint": "eslint src",
    "prettier": "prettier --single-quote --trailing-comma es5 --write \"src/**/*.js\"",
    "prepublishOnly": "npm run lint && npm run test && npm run build",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "deploy": "npm run build:demo && gh-pages -d build"
  },
  "main": "dist/main.js",
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/fritz-c/react-sortable-tree"
  },
  "homepage": "https://fritz-c.github.io/react-sortable-tree",
  "bugs": "https://github.com/fritz-c/react-sortable-tree/issues",
  "authors": [
    "Chris Fritz"
  ],
  "license": "MIT",
  "jest": {
    "setupTestFrameworkScriptFile": "./node_modules/jest-enzyme/lib/index.js",
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "moduleDirectories": [
      "node_modules"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|scss|less)$": "identity-obj-proxy"
    }
  },
  "dependencies": {
    "lodash.isequal": "^4.4.0",
    "prop-types": "^15.5.8",
    "react-dnd": "^2.1.4",
    "react-dnd-html5-backend": "^2.1.2",
    "react-dnd-scrollzone": "^4.0.0",
    "react-virtualized": "^9.9.0"
  },
  "peerDependencies": {
    "react": "^15.3.0",
    "react-dom": "^15.3.0"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.2",
    "babel-cli": "^6.10.1",
    "babel-core": "^6.10.4",
    "babel-eslint": "^7.0.0",
    "babel-jest": "^20.0.3",
    "babel-loader": "^7.1.1",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.11.1",
    "cross-env": "^5.0.1",
    "css-loader": "^0.28.4",
    "enzyme": "^2.9.1",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.0.2",
    "eslint-config-prettier": "^2.3.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.1.0",
    "file-loader": "^0.11.2",
    "gh-pages": "^1.0.0",
    "html-webpack-plugin": "^2.29.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^20.0.4",
    "jest-enzyme": "^3.5.3",
    "json-loader": "^0.5.4",
    "node-sass": "^4.0.0",
    "postcss-loader": "^2.0.6",
    "prettier": "^1.5.3",
    "react": "^15.6.1",
    "react-addons-shallow-compare": "^15.3.0",
    "react-addons-test-utils": "^15.4.1",
    "react-dom": "^15.6.1",
    "react-hot-loader": "^3.0.0-beta.7",
    "react-test-renderer": "^15.6.1",
    "rimraf": "^2.5.3",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.18.2",
    "webpack": "^3.3.0",
    "webpack-dev-server": "^2.5.1",
    "webpack-node-externals": "^1.6.0"
  },
  "keywords": [
    "react",
    "react-component"
  ]
}
